{
    "name": "waygou/web-command",
    "description": "Run console commands via a web form in Laravel.",
    "type": "library",
    "keywords": [
        "utilities",
        "waygou",
        "laravel",
        "php"
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "5.6.*|5.7.*",
        "symfony/process": "^4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Waygou\\WebCommand\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Waygou\\WebCommand\\WebCommandServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Waygou",
            "email": "bruno.falcao@waygou.com"
        }
    ],
    "minimum-stability": "dev"
}
